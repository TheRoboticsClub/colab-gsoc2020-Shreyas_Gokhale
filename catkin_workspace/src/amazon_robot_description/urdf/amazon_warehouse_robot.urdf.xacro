  <link name="link_platform_mid">
    <origin xyz="0 0 0.0" rpy="0 0 0"/>

    <visual>
      <geometry>
        <box size="0.02 0.02 0.10" />
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.02 0.02 0.10" />
      </geometry>
    </collision>

    <inertial>
      <mass value="1"/>
      <inertia
              ixx="1.0" ixy="0.0" ixz="0.0"
              iyy="1.0" iyz="0.0"
              izz="1.0"/>
    </inertial>

  </link>
  <joint name="platform_joint" type="fixed">
    <parent link="base_link" />
    <child link="link_platform_mid" />
    <origin xyz="0.0 0 0.20" rpy="0. 0. 0."/>
  </joint>


  <link name="lift_link">
    <origin xyz="0 0 0.0" rpy="0 0 0"/>

    <visual>
      <geometry>
        <box size="0.40 0.40 0.01" />
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.40 0.40 0.01" />
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia
              ixx="1.0" ixy="0.0" ixz="0.0"
              iyy="1.0" iyz="0.0"
              izz="1.0"/>
    </inertial>
  </link>

